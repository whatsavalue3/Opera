@#8 CPU_BITS = 16;

$ int #((CPU_BITS == 32) ? 32 : 16);
$ void !;

& void comprint(int a) (%a);;

& void testfunca(int arga, int argb)
(
	((arga - argb) - argb) ? 
	(
		comprint(arga + argb);
	)
	:
	(
		comprint(arga - argb);
	)
);;

& void testfunc(int argo)
(
	testfunca(argo, 123);
);;

(CPU_BITS == 16) ?
(
	& void main()
	(
		testfunc(123);
		testfunc(246);
		testfunc(456);
	)
)
:
(
	& void main()
	(
		testfunc(246);
		testfunc(246);
		testfunc(246);
	)
)